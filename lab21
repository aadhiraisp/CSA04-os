#include <stdio.h>

int main() {
    int disk[50] = {0}, start, length;

    printf("Enter starting block: ");
    scanf("%d", &start);
    printf("Enter file length: ");
    scanf("%d", &length);

    printf("\nAllocated Blocks:\n");
    for (int i = start; i < start + length; i++) {
        if (disk[i] == 0) {
            disk[i] = 1;
            printf("%d ", i);
        }
    }
    return 0;
}
